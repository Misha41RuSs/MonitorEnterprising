<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="519.0" prefWidth="1019.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kp.monitorenterprising.Controller.MyOrdersController">

    <children>
        <Label layoutX="412.0" layoutY="27.0" text="Мои заказы">
            <font>
                <Font name="Times New Roman Bold" size="36.0" />
            </font>
        </Label>

        <TableView fx:id="ordersTable" layoutX="60.0" layoutY="80.0" prefHeight="350.0" prefWidth="900.0">
            <columns>
                <TableColumn fx:id="orderIdColumn" prefWidth="80.0" text="ID" />
                <TableColumn fx:id="modelColumn" prefWidth="200.0" text="Модель" />
                <TableColumn fx:id="quantityColumn" prefWidth="120.0" text="Количество" />
                <TableColumn fx:id="orderDateColumn" prefWidth="200.0" text="Дата" />
                <TableColumn fx:id="totalPriceColumn" prefWidth="150.0" text="Сумма" />
                <TableColumn fx:id="statusColumn" prefWidth="150.0" text="Статус"/>
            </columns>
        </TableView>

        <Button layoutX="60.0" layoutY="450.0" onAction="#goBack" prefHeight="40.0" prefWidth="120.0" text="Назад">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Button>
    </children>
</Pane>
